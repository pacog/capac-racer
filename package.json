{
    "name": "capac-racer",
    "version": "1.1.1",
    "private": false,
    "homepage": "https://pacog.github.io/capac-racer",
    "dependencies": {
        "classnames": "^2.2.6",
        "framer-motion": "^1.10.3",
        "lodash.debounce": "^4.0.8",
        "lodash.shuffle": "^4.2.0",
        "pluralize": "^8.0.0",
        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-redux": "^7.1.3",
        "react-resize-detector": "^4.2.1",
        "react-scripts": "^3.4.1",
        "react-svg-pathline": "^0.5.0",
        "react-transition-group": "^4.3.0",
        "redux": "^4.0.5",
        "redux-devtools-extension": "^2.13.8",
        "redux-thunk": "^2.3.0",
        "uuid": "^7.0.3"
    },
    "scripts": {
        "analyze": "source-map-explorer 'build/static/js/*.js'",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "start": "REACT_APP_VERSION=$npm_package_version react-scripts start",
        "build": "REACT_APP_VERSION=$npm_package_version react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "e2e": "jest -c e2e/jest.config.js",
        "typecheck": "tsc --baseUrl ./src --jsx react -allowJs -checkJs -noEmit -allowSyntheticDefaultImports -moduleResolution node -target ES6 src/index.js",
        "lint": "eslint src/**/*.js"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/expect-puppeteer": "^4.4.1",
        "@types/jest-environment-puppeteer": "^4.3.1",
        "@types/puppeteer": "^2.0.1",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.19.0",
        "gh-pages": "^2.2.0",
        "jest-image-snapshot": "^3.0.1",
        "jest-puppeteer": "^4.4.0",
        "prettier": "^2.0.2",
        "puppeteer": "^2.1.1",
        "source-map-explorer": "^2.4.2",
        "typescript": "^3.8.3"
    }
}
